<h1>Editing user_archive</h1>

<% form_for(@user_archive) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :usr_id %><br />
    <%= f.text_field :usr_id %>
  </p>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_area :state %>
  </p>
  <p>
    <%= f.label :usr_created_at %><br />
    <%= f.datetime_select :usr_created %>
  </p>
  <p>
    <%= f.label :usr_updated_at %><br />
    <%= f.datetime_select :usr_updated %>
  </p>
  <p>
    <%= f.label :archive_time %><br />
    <%= f.text_field :archive_time %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @user_archive %> |
<%= link_to 'Back', user_archives_path %>